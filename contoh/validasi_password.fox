# contoh/validasi_password.fox
# Program untuk memvalidasi panjang sebuah password.

biar password = "kata_sandi_rahasia"
biar panjang_minimal = 8

biar panjang_password = panjang(password)

jika panjang_password >= panjang_minimal maka
    tulis("Password valid. Panjangnya cukup.")
akhir

jika panjang_password < panjang_minimal maka
    tulis("Password terlalu pendek. Harus minimal 8 karakter.")
akhir
