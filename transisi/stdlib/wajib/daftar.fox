// --- Fungsi Dasar (via FFI) ---

fungsi panjang(daftar) maka
    kembalikan internal.panjang_daftar(daftar)
akhir

fungsi tambah(daftar, item) maka
    kembalikan internal.tambah_ke_daftar(daftar, item)
akhir

fungsi hapus(daftar, indeks) maka
    kembalikan internal.hapus_dari_daftar(daftar, indeks)
akhir

fungsi urut(daftar) maka
    kembalikan internal.urut_daftar(daftar)
akhir

fungsi balik(daftar) maka
